<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.jeson.baiduapi">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/app"
        android:label="JZhiFu"
        android:supportsRtl="true"
        android:theme="@style/MyMaterialTheme">
        <activity
            android:name=".activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".activity.NewsDetailsActivity"
            android:label="News"
            android:theme="@style/NewsDetailsTheme"
            />
        <activity android:name=".activity.PicDetailsActivity"
            android:theme="@style/NewsDetailsTheme"
            android:label="Picture"></activity>
    </application>

</manifest>